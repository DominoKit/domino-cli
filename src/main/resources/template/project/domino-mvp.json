{
  "name": "${artifactId}",
  "condition": "true",
  "folders": [
    {
      "name": "${name}-frontend",
      "condition": "true",
      "folders": [
        {
          "name": "src",
          "condition": "true",
          "folders": [
            {
              "name": "main",
              "condition": "true",
              "folders": [
                {
                  "name": "java.${rootPackage}",
                  "condition": "true",
                  "folders": [],
                  "files": [
                    {
                      "name": "AppClientModule.java",
                      "template": "app/mvp/__name__-frontend/src/main/java.__rootPackage__/AppClientModule.java",
                      "type": "VELOCITY"
                    }
                  ]
                }
              ],
              "files": [
                {
                  "name": "module.gwt.xml",
                  "template": "app/mvp/__name__-frontend/src/main/module.gwt.xml",
                  "type": "VELOCITY"
                }
              ]
            }
          ],
          "files": []
        }
      ],
      "files": [
        {
          "name": "pom.xml",
          "template": "app/mvp/__name__-frontend/pom.xml",
          "type": "VELOCITY"
        }
      ]
    },
    {
      "name": "${name}-backend",
      "condition": "true",
      "folders": [
        {
          "name": "src",
          "condition": "true",
          "folders": [
            {
              "name": "main",
              "condition": "true",
              "folders": [
                {
                  "name": "resources",
                  "condition": "true",
                  "folders": [
                    {
                      "name": "app",
                      "condition": "true",
                      "folders": [],
                      "files": [
                        {
                          "name": "${name}.css",
                          "template": "app/mvp/__name__-backend/src/main/resources/app/__name__.css",
                          "type": "VELOCITY"
                        },
                        {
                          "name": "favicon.ico",
                          "template": "app/mvp/__name__-backend/src/main/resources/app/favicon.ico",
                          "type": "RESOURCE"
                        },
                        {
                          "name": "index.html",
                          "template": "app/mvp/__name__-backend/src/main/resources/app/index.html",
                          "type": "VELOCITY"
                        }
                      ]
                    }
                  ],
                  "files": [
                    {
                      "name": "config.json",
                      "template": "app/mvp/__name__-backend/src/main/resources/config.json",
                      "type": "VELOCITY"
                    }
                  ]
                }
              ],
              "files": []
            }
          ],
          "files": []
        }
      ],
      "files": [
        {
          "name": "pom.xml",
          "template": "app/mvp/__name__-backend/pom.xml",
          "type": "VELOCITY"
        }
      ]
    },
    {
      "name": ".idea",
      "condition": "true",
      "folders": [
        {
          "name": "runConfigurations",
          "condition": "true",
          "folders": [],
          "files": [
            {
              "name": "process_sources.xml",
              "template": "app/mvp/.idea/runConfigurations/process_sources.xml",
              "type": "VELOCITY"
            },
            {
              "name": "js_debug.xml",
              "template": "app/mvp/.idea/runConfigurations/js_debug.xml",
              "type": "VELOCITY"
            },
            {
              "name": "${name}-dev.xml",
              "template": "app/mvp/.idea/runConfigurations/__name__-dev.xml",
              "type": "VELOCITY"
            },
            {
              "name": "ALL_TESTS.xml",
              "template": "app/mvp/.idea/runConfigurations/ALL_TESTS.xml",
              "type": "VELOCITY"
            },
            {
              "name": "super_dev_mode.xml",
              "template": "app/mvp/.idea/runConfigurations/super_dev_mode.xml",
              "type": "VELOCITY"
            },
            {
              "name": "${name}.xml",
              "template": "app/mvp/.idea/runConfigurations/__name__.xml",
              "type": "VELOCITY"
            },
            {
              "name": "Development.xml",
              "template": "app/mvp/.idea/runConfigurations/Development.xml",
              "type": "VELOCITY"
            }
          ]
        }
      ],
      "files": []
    }
  ],
  "files": [
    {
      "name": "pom.xml",
      "template": "app/mvp/pom.xml",
      "type": "VELOCITY"
    },
    {
      "name": "README.md",
      "template": "app/mvp/README.md",
      "type": "VELOCITY"
    }
  ]
}